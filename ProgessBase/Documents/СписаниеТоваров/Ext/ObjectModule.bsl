	НС = ПРГ_ПолучитьНомерСубконто(Счет94,ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ПРГ_ВидыТМЦ);
	Если НС > 0 Тогда
		СтруктураПолей.Вставить("КорСубконто"+НС, "ВидТмц");
	КонецЕсли;	
	НС = ПРГ_ПолучитьНомерСубконто(Счет94,ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.РаботникиОрганизации);
	Если НС > 0 Тогда
		СтруктураПолей.Вставить("КорСубконто"+НС, "Ссылка.ФизЛицо");
	КонецЕсли;	
	//конец изменений 
	

КонецПроцедуры

