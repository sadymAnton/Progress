// Процедура вызывается перед записью документа 
//
Процедура ПередЗаписью(Отказ, РежимЗаписи, РежимПроведения)
	
	Если ОбменДанными.Загрузка  Тогда
		Возврат;
	КонецЕсли;

	//начало изменений Ожиганов 28.05.2015 немножко оптимизируем 
	ПРГ_ДопФункцииКлиентСервер.ДобавитьВОбъектСвойстваДляУдаленияДвижения(ЭтотОбъект,РежимЗаписи);
	//конец изменений 	
	// Проверка заполнения единицы измерения мест и количества мест
	ОбработкаТабличныхЧастей.ПриЗаписиПроверитьЕдиницуИзмеренияМест(Товары);
	//начало изменений  
	Если Дата >= '20140401' Тогда
		Если ОтражатьВБухгалтерскомУчете Тогда
			ПРГ_ТаблСчетов = ПРГ_ДопФункцииКлиентСервер.ПолучитьТаблСчетовВидовТмц();
			Для Каждого ТекСтрока  Из Товары Цикл
				Если Не ЗначениеЗаполнено(ТекСтрока.ВидТМЦ) Тогда
					найдзнч = ПРГ_ТаблСчетов.Найти(ТекСтрока.СчетУчетаБУ,"Счет");
					Если найдзнч  <> Неопределено Тогда
						ТекСтрока.ВидТМЦ = найдзнч.ВидТмц;
					КонецЕсли;	
				КонецЕсли;	
			КонецЦикла;	
			
			Для Каждого ТекСтрока  Из ВозвратнаяТара Цикл
				Если Не ЗначениеЗаполнено(ТекСтрока.ВидТМЦ) Тогда
					найдзнч = ПРГ_ТаблСчетов.Найти(ТекСтрока.СчетУчетаБУ,"Счет");
					Если найдзнч  <> Неопределено Тогда
						ТекСтрока.ВидТМЦ = найдзнч.ВидТмц;
					КонецЕсли;	
				КонецЕсли;	
			КонецЦикла;	
			Если Не ЗначениеЗаполнено(ФизЛицо) Тогда
				ФизЛицо = ПолучитьОтветственноеЛицо();
			КонецЕсли;	
		КонецЕсли;		
	КонецЕсли;	
	//конец изменений 

	// Посчитать суммы документа и записать ее в соответствующий реквизит шапки для показа в журналах
	СуммаДокумента = Товары.Итог("Сумма");

	// Почистим неиспользуемые реквизиты.
	Если ВидОперации = Перечисления.ВидыОперацийСписаниеТоваров.Оборудование Тогда 

		Для каждого СтрокаТаблицы Из ЭтотОбъект.Товары Цикл

			Если ЗначениеЗаполнено(СтрокаТаблицы.ДокументРезерва) Тогда
				СтрокаТаблицы.ДокументРезерва = Неопределено;
			КонецЕсли;

			Если СтрокаТаблицы.Качество <> Справочники.Качество.Новый Тогда
				СтрокаТаблицы.Качество = Справочники.Качество.Новый;
			КонецЕсли;

		КонецЦикла;
	Иначе
		ОбработкаТабличныхЧастей.ЗаполнитьКачествоПоУмолчанию(Товары);
	КонецЕсли;

	мУдалятьДвижения = НЕ ЭтоНовый();
	//начало изменений
	Если ОтражатьВНалоговомУчете и ЗначениеЗаполнено(СчетЗатратШапка)
		и не ЗначениеЗаполнено(СчетЗатратНУШапка)
		Тогда
		Если СчетЗатратШапка = ПланыСчетов.Хозрасчетный.РасчетыПоВозмещениюМатериальногоУщерба
		 или СчетЗатратШапка = ПланыСчетов.Хозрасчетный.РасчетыПоПрочимОперациям Тогда
		     СчетЗатратНУШапка = ПланыСчетов.Налоговый.ПоступлениеИВыбытиеИмуществаРаботУслугПрав;
		ИначеЕсли СчетЗатратШапка = ПланыСчетов.Хозрасчетный.ПрочиеРасходыНеОблагаемыеЕНВД Тогда
			 СчетЗатратНУШапка = ПланыСчетов.Налоговый.ВнереализационныеРасходы;
			 СубконтоНУШапка1  = СубконтоШапка1;
		КонецЕсли; 
	КонецЕсли;	
	//конец изменений
	
	//m.ionov@a-prof.ru 08.10.2014
	Если НЕ ДоступенСтатусДокумента() Тогда
		ПР_СтатусПриходногоОрдера = Перечисления.СП_СтатусыПриходногоОрдераСолво.ПустаяСсылка();
	КонецЕсли;
	
КонецПроцедуры // ПередЗаписью

// Процедура - обработчик события "ПриЗаписи"
