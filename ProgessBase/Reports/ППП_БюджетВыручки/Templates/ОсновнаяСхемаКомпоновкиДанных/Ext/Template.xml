		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Контрагенты.Дивизион КАК Дивизион,
	НСИ_Брендообъемы.Ссылка КАК Брендообъем,
	ВЫБОР
		КОГДА Контрагенты.Дивизион.Код = "ЗАРУБЕЖЬЕ_ПРГ"
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС0)
		ИНАЧЕ НСИ_Брендообъемы.СтавкаНДС
	КОНЕЦ КАК СтавкаНДС,
	ВЫБОР
		КОГДА Контрагенты.Дивизион.Код = "ЗАРУБЕЖЬЕ_ПРГ"
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС18_118)
						ИЛИ НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС18)
					ТОГДА 0.18
				КОГДА НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС10_110)
						ИЛИ НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС10)
					ТОГДА 0.1
				КОГДА НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20_120)
						ИЛИ НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20)
					ТОГДА 0.2
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ КАК СтавкаНДСЧислом
ПОМЕСТИТЬ втДивизионыИБрендообъемы
ИЗ
	Справочник.Контрагенты КАК Контрагенты,
	Справочник.НСИ_Брендообъемы КАК НСИ_Брендообъемы
ГДЕ
	Контрагенты.Покупатель

СГРУППИРОВАТЬ ПО
	Контрагенты.Дивизион,
	НСИ_Брендообъемы.Ссылка,
	ВЫБОР
		КОГДА Контрагенты.Дивизион.Код = "ЗАРУБЕЖЬЕ_ПРГ"
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС0)
		ИНАЧЕ НСИ_Брендообъемы.СтавкаНДС
	КОНЕЦ,
	ВЫБОР
		КОГДА Контрагенты.Дивизион.Код = "ЗАРУБЕЖЬЕ_ПРГ"
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС18_118)
						ИЛИ НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС18)
					ТОГДА 0.18
				КОГДА НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС10_110)
						ИЛИ НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС10)
					ТОГДА 0.1
				КОГДА НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20_120)
						ИЛИ НСИ_Брендообъемы.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20)
					ТОГДА 0.2
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ

ИНДЕКСИРОВАТЬ ПО
	Дивизион,
	Брендообъем
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПланыПродаж.Период,
	ПланыПродаж.Контрагент,
	ПланыПродаж.ППП_АдресПоставки,
	ПланыПродаж.Контрагент.НСИ_ТорговаяКоманда КАК НСИ_ТорговаяКоманда,
	ПланыПродаж.Контрагент.НСИ_КаналПродаж КАК НСИ_КатегорияКлиента,
	ВЫБОР
		КОГДА ПланыПродаж.ППП_АдресПоставки.Дивизион &lt;&gt; ЗНАЧЕНИЕ(Справочник.ПРГДивизионы.ПустаяСсылка)
			ТОГДА ПланыПродаж.ППП_АдресПоставки.Дивизион
		ИНАЧЕ ПланыПродаж.Контрагент.Дивизион
	КОНЕЦ КАК Дивизион,
	ПланыПродаж.ППП_ТоварнаяКатегория,
	ПланыПродаж.ППП_Брендообъем КАК ППП_Брендообъем,
	ПланыПродаж.ППП_АдресПоставки.НСИ_КатегорияКлиента КАК ППП_КаналПродаж,
