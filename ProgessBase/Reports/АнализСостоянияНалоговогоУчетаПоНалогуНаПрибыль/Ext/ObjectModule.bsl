		ОбластьОтчета.Параметры.Заполнить(Выборка);
		
		СоздатьРамку(Выборка, "КосвенныеРасходыУч", ОбластьОтчета, 13, 37, 60, 2, "РасшифровкаКосвенныеРасходыУч");	
