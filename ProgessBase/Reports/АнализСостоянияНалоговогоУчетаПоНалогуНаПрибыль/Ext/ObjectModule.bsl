		|			0,
		|			0,
		|			0,
		|			0,
		|			0,
		|			0,
		|			0,
		|			ВЫБОР
		|				КОГДА НалоговыйОстатки.ВидУчета = &НУ
		|					ТОГДА НалоговыйОстатки.СуммаОстаток
		|				ИНАЧЕ 0
		|			КОНЕЦ,
		|			ВЫБОР
		|				КОГДА НалоговыйОстатки.ВидУчета = &ПР
		|					ТОГДА НалоговыйОстатки.СуммаОстаток
		|				ИНАЧЕ 0
		|			КОНЕЦ,
		|			ВЫБОР
		|				КОГДА НалоговыйОстатки.ВидУчета = &ВР
		|					ТОГДА НалоговыйОстатки.СуммаОстаток
		|				ИНАЧЕ 0
		|			КОНЕЦ
		|		ИЗ
		|			РегистрБухгалтерии.Налоговый.Остатки(
		|					&КонецПериодаТек2,
		|					Счет В (&СчетаКосвенныхРасходовНеЕНВДНалоговый),
		|					&СтатьиЗатрат,
		|					Организация = &Организация
		|						И (НЕ Субконто1.ВидРасходовНУ = &ТранспортныеРасходы)) КАК НалоговыйОстатки) КАК ВложенныйЗапрос
		|	
		|	СГРУППИРОВАТЬ ПО
		|		ВложенныйЗапрос.Организация) КАК ВложенныйЗапрос2
		|
		|СГРУППИРОВАТЬ ПО
		|	ВложенныйЗапрос2.Организация";
		
		Результат = Запрос.Выполнить();
		Выборка = Результат.Выбрать();
