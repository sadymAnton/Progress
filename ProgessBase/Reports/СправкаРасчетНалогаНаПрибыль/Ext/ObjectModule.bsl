#Если Клиент Тогда
	
	Перем НП Экспорт;
	Перем ОбластьОкончаниеТаблицы;
	Перем ПоддержкаПБУ18;
	Перем ЭтоУПП;
	
	Функция СформироватьТекстЗапроса(ВидЗапроса, ВидУчета)
		
		Если Не Найти(ВидЗапроса, "ОсновнаяДеятельность") = 0 Тогда	
			
			Текст	= "ВЫБРАТЬ
			     	  |	ДоходыИРасходы.Субконто1 КАК Показатель,
			     	  |	ЕСТЬNULL(Доходы.ДоходГод, 0) КАК ДоходГод,
			     	  |	ЕСТЬNULL(Доходы.ДоходМесяц, 0) КАК ДоходМесяц,
			     	  |	ЕСТЬNULL(Расходы.РасходГод, 0) КАК РасходГод,
			     	  |	ЕСТЬNULL(Расходы.РасходМесяц, 0) КАК РасходМесяц
			     	  |ИЗ
			     	  |	РегистрБухгалтерии.Хозрасчетный.Обороты(
			     	  |			&ДатаНачГода,
			     	  |			&ДатаКон,
			     	  |			Период,
			     	  |			Счет В ИЕРАРХИИ (&СчетДоходов)
			     	  |				ИЛИ Счет В ИЕРАРХИИ (&СчетРасходов),
			     	  |			,
			     	  |			Организация = &Организация,
			     	  |			(НЕ КорСчет = &Счет09)
			     	  |				И (НЕ КорСчет = &СчетПустой),
			     	  |			) КАК ДоходыИРасходы
			     	  |		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			     	  |			РасходыГод.СубконтоДт1 КАК Показатель,
			     	  |			РасходыГод.СуммаОборот КАК РасходГод,
			     	  |			РасходыМесяц.Расходы КАК РасходМесяц
			     	  |		ИЗ
			     	  |			РегистрБухгалтерии.Хозрасчетный.ОборотыДтКт(
			     	  |					&ДатаНачГода,
			     	  |					&ДатаКон,
			     	  |					Период,
			     	  |					СчетДт В ИЕРАРХИИ (&СчетРасходов),
			     	  |					,
			     	  |					(НЕ СчетКт = &Счет09)
			     	  |						И (НЕ СчетКт = &СчетПустой),
			     	  |					,
			     	  |					Организация = &Организация) КАК РасходыГод
			     	  |				ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			     	  |					Расходы.СубконтоДт1 КАК Показатель,
			     	  |					Расходы.СуммаОборот КАК Расходы
			     	  |				ИЗ
			     	  |					РегистрБухгалтерии.Хозрасчетный.ОборотыДтКт(
			     	  |							&ДатаНач,
			     	  |							&ДатаКон,
			     	  |							,
			     	  |							СчетДт В ИЕРАРХИИ (&СчетРасходов),
			     	  |							,
			     	  |							(НЕ СчетКт = &Счет09)
			     	  |								И (НЕ СчетКт = &СчетПустой),
			     	  |							,
			     	  |							Организация = &Организация) КАК Расходы) КАК РасходыМесяц
			     	  |				ПО РасходыГод.СубконтоДт1 = РасходыМесяц.Показатель) КАК Расходы
			     	  |		ПО ДоходыИРасходы.Субконто1 = Расходы.Показатель
			     	  |		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			     	  |			ДоходыГод.Субконто1 КАК Показатель,
			     	  |			ДоходыГод.СуммаОборотКт - ДоходыГод.СуммаОборотДт КАК ДоходГод,
			     	  |			ДоходыМесяц.Доход КАК ДоходМесяц
			     	  |		ИЗ
			     	  |			РегистрБухгалтерии.Хозрасчетный.Обороты(
			     	  |					&ДатаНачГода,
			     	  |					&ДатаКон,
			     	  |					Период,
			     	  |					Счет В ИЕРАРХИИ (&СчетДоходов),
			     	  |					,
			     	  |					Организация = &Организация,
			     	  |					(НЕ КорСчет = &Счет09)
			     	  |						И (НЕ КорСчет = &СчетПустой),
			     	  |					) КАК ДоходыГод
			     	  |				ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			     	  |					Доходы.Субконто1 КАК Показатель,
			     	  |					Доходы.СуммаОборотКт - Доходы.СуммаОборотДт КАК Доход
			     	  |				ИЗ
			     	  |					РегистрБухгалтерии.Хозрасчетный.Обороты(
			     	  |							&ДатаНач,
			     	  |							&ДатаКон,
			     	  |							,
			     	  |							Счет В ИЕРАРХИИ (&СчетДоходов),
			     	  |							,
			     	  |							Организация = &Организация,
			     	  |							(НЕ КорСчет = &Счет09)
			     	  |								И (НЕ КорСчет = &СчетПустой),
			     	  |							) КАК Доходы) КАК ДоходыМесяц
			     	  |				ПО ДоходыГод.Субконто1 = ДоходыМесяц.Показатель) КАК Доходы
			     	  |		ПО ДоходыИРасходы.Субконто1 = Доходы.Показатель
			     	  |ИТОГИ
			     	  |	СУММА(ДоходГод),
			     	  |	СУММА(ДоходМесяц),
			     	  |	СУММА(РасходГод),
			     	  |	СУММА(РасходМесяц)
			     	  |ПО
			     	  |	ОБЩИЕ,
			     	  |	Показатель";
			
			Если Не ВидУчета = "БУ" Тогда
				Текст = СтрЗаменить(Текст, "Организация = &Организация, , )", "ВидУчета = &ВидУчета И Организация = &Организация, , )");
				Текст = СтрЗаменить(Текст, "Организация = &Организация)", "ВидУчетаДт = &ВидУчета И Организация = &Организация)");
				Текст = СтрЗаменить(Текст, "Организация = &Организация,", "ВидУчета = &ВидУчета И Организация = &Организация,");
				Текст = СтрЗаменить(Текст, "Хозрасчетный", "Налоговый");
			КонецЕсли;
			
			Если ВидУчета = "НУ" И ВидЗапроса = "ОсновнаяДеятельность" Тогда
				Текст = СтрЗаменить(Текст, "Субконто1", "Счет");
				Текст = СтрЗаменить(Текст, "СубконтоДт1", "СчетДт");
			КонецЕсли;
			
			Если ВидЗапроса = "НеОсновнаяДеятельность" И Не ЭтоУПП Тогда
				Текст = СтрЗаменить(Текст, "Организация = &Организация, , )", "Субконто1.ОтнесениеРасходовКДеятельностиЕНВД = &ННП И Организация = &Организация, , )");
				Текст = СтрЗаменить(Текст, "Организация = &Организация)", "СубконтоДт1.ОтнесениеРасходовКДеятельностиЕНВД = &ННП И Организация = &Организация)");
				Текст = СтрЗаменить(Текст, "(&СчетДоходов), , Организация = &Организация", "(&СчетДоходов), , Организация = &Организация И Субконто1.ОтнесениеРасходовКДеятельностиЕНВД = &ННП");
			КонецЕсли;
			
			Возврат Текст;
			
		КонецЕсли;	
		
		Если ВидЗапроса = "НалогНаПрибыль" Тогда	
			
			Текст	= "ВЫБРАТЬ
			     	  |	НалогГод.КорСчет КАК КорСчет,
			     	  |	ЕСТЬNULL(-НалогГод.СуммаОборот, 0) КАК НалогГод,
			     	  |	ЕСТЬNULL(-НалогМесяц.Сумма, 0) КАК НалогМесяц,
			     	  |	ЕСТЬNULL(-НалогГод.СуммаОборот, 0) + ЕСТЬNULL(НалогМесяц.Сумма, 0) КАК Налог,
			     	  |	ВЫБОР
			     	  |		КОГДА НалогГод.КорСчет В ИЕРАРХИИ (&СчетНалога)
			     	  |			ТОГДА 1
			     	  |		ИНАЧЕ ВЫБОР
			     	  |				КОГДА НалогГод.КорСчет В ИЕРАРХИИ (&СчетПР)
			     	  |					ТОГДА 2
			     	  |				ИНАЧЕ 3
			     	  |			КОНЕЦ
			     	  |	КОНЕЦ КАК Порядок
			     	  |ИЗ
			     	  |	РегистрБухгалтерии.Хозрасчетный.Обороты(&ДатаНачГода, &ДатаКон, Месяц, Счет В ИЕРАРХИИ (&Счет), , Организация = &Организация И Субконто1 = &Налоги, (НЕ КорСчет В ИЕРАРХИИ (&Счет)), ) КАК НалогГод
			     	  |		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			     	  |			НалогМесяц.КорСчет КАК КорСчет,
			     	  |			Сумма(НалогМесяц.СуммаОборот) КАК Сумма
			     	  |		ИЗ
			     	  |			РегистрБухгалтерии.Хозрасчетный.Обороты(&ДатаНач, &ДатаКон, Месяц, Счет В ИЕРАРХИИ (&Счет), , Организация = &Организация И Субконто1 = &Налоги, (НЕ КорСчет В ИЕРАРХИИ (&Счет)), ) КАК НалогМесяц
					  | СГРУППИРОВАТЬ ПО
	                  |  НалогМесяц.КорСчет) КАК НалогМесяц
			     	  |		ПО НалогГод.КорСчет = НалогМесяц.КорСчет
			     	  |
					  |УПОРЯДОЧИТЬ ПО
			     	  |	Порядок
			     	  |ИТОГИ
			     	  |ПО
			     	  |	ОБЩИЕ,
			     	  |	КорСчет";
			
			
			Если ВариантОтчета = 1 Тогда
				Текст = СтрЗаменить(Текст, "НЕ КорСчет В ИЕРАРХИИ (&Счет)", "КорСчет В ИЕРАРХИИ (&СчетНалога)");
			Иначе 
				Текст = СтрЗаменить(Текст, "НЕ КорСчет В ИЕРАРХИИ (&Счет)", "НЕ КорСчет В ИЕРАРХИИ (&Счет) И НЕ КорСчет В ИЕРАРХИИ (&СчетаИсключения)");
			КонецЕсли;
			Если ПоддержкаПБУ18 Тогда
				Текст = СтрЗаменить(Текст, " И Субконто1 = &Налоги", "");
			КонецЕсли;
			Возврат Текст;
		КонецЕсли;
		
		Если ВидЗапроса = "УбыткиПрошлыхЛет" Тогда	
			
			Текст	= "ВЫБРАТЬ
			     	  |	УбытокГод.Счет КАК СчетУбытка,
			     	  |	УбытокГод.Субконто1 КАК Показатель,
			     	  |	ЕСТЬNULL(УбытокГод.СуммаОборотДт, 0) КАК ДоходГод,
			     	  |	ЕСТЬNULL(УбытокГод.СуммаОборотКт, 0) КАК РасходГод,
			     	  |	НалогМесяц.Перенесен КАК ДоходМесяц,
			     	  |	НалогМесяц.Списан КАК РасходМесяц
			     	  |ИЗ
			     	  |	РегистрБухгалтерии.Налоговый.Обороты(&ДатаНачГода, &ДатаКон, Период, Счет В ИЕРАРХИИ (&Счет97), , Организация = &Организация И ВидУчета = &ВидУчета, , ) КАК УбытокГод
			     	  |		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			     	  |			УбытокМесяц.Счет КАК СчетУбытка,
			     	  |			ЕСТЬNULL(УбытокМесяц.СуммаОборотДт, 0) КАК Перенесен,
			     	  |			ЕСТЬNULL(УбытокМесяц.СуммаОборотКт, 0) КАК Списан,
			     	  |			УбытокМесяц.Субконто1 КАК Убыток
			     	  |		ИЗ
			     	  |			РегистрБухгалтерии.Налоговый.Обороты(&ДатаНач, &ДатаКон, Месяц, Счет В ИЕРАРХИИ (&Счет97), , Организация = &Организация И ВидУчета = &ВидУчета, , ) КАК УбытокМесяц) КАК НалогМесяц
			     	  |		ПО УбытокГод.Счет = НалогМесяц.СчетУбытка
			     	  |			И УбытокГод.Субконто1 = НалогМесяц.Убыток
			     	  |ИТОГИ
			     	  |	СУММА(ДоходГод),
			     	  |	СУММА(РасходГод),
			     	  |	СУММА(ДоходМесяц),
			     	  |	СУММА(РасходМесяц)
			     	  |ПО
			     	  |	ОБЩИЕ,
			     	  |	Показатель";
			
			
			Возврат Текст;
		КонецЕсли;
		
		Если ВидЗапроса = "Распределяемые расходы" Тогда	
			
			Текст	= "ВЫБРАТЬ
			     	  |	ДоходыГод.Субконто1 КАК Показатель,
			     	  |	СУММА(ЕСТЬNULL(-ДоходыГод.СуммаОборотДт, 0)) КАК ДоходГод,
			     	  |	СУММА(-ЕСТЬNULL(ДоходыМесяц.Доход, 0)) КАК ДоходМесяц,
			     	  |	СУММА(0) КАК РасходГод,
			     	  |	СУММА(0) КАК РасходМесяц
			     	  |ИЗ
			     	  |	РегистрБухгалтерии.Хозрасчетный.Обороты(&ДатаНачГода, &ДатаКон, Период, Счет В ИЕРАРХИИ (&СчетРасходов), , Организация = &Организация, , ) КАК ДоходыГод
			     	  |		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			     	  |			Доходы.Субконто1 КАК Показатель,
			     	  |			ЕСТЬNULL(Доходы.СуммаОборотДт, 0) КАК Доход
			     	  |		ИЗ
			     	  |			РегистрБухгалтерии.Хозрасчетный.Обороты(&ДатаНач, &ДатаКон, , Счет В ИЕРАРХИИ (&СчетРасходов), , Организация = &Организация, , ) КАК Доходы) КАК ДоходыМесяц
			     	  |		ПО ДоходыГод.Субконто1 = ДоходыМесяц.Показатель
			     	  |
			     	  |СГРУППИРОВАТЬ ПО
			     	  |	ДоходыГод.Субконто1
			     	  |
			     	  |ОБЪЕДИНИТЬ ВСЕ
			     	  |
			     	  |ВЫБРАТЬ
			     	  |	""ЕНВД"",
			     	  |	0,
			     	  |	0,
			     	  |	СУММА(ЕСТЬNULL(-РасходыГод.СуммаОборот, 0)),
			     	  |	0
			     	  |ИЗ
			     	  |	РегистрБухгалтерии.Хозрасчетный.Обороты(&ДатаНачГода, &ДатаКон, , Счет В ИЕРАРХИИ (&СчетДоходов), , Организация = &Организация, КорСчет В ИЕРАРХИИ (&Счет91_09), ) КАК РасходыГод
			     	  |
			     	  |ОБЪЕДИНИТЬ ВСЕ
			     	  |
			     	  |ВЫБРАТЬ
			     	  |	""ЕНВД"",
			     	  |	0,
			     	  |	0,
			     	  |	0,
			     	  |	СУММА(ЕСТЬNULL(-РасходыМесяц.СуммаОборот, 0))
			     	  |ИЗ
			     	  |	РегистрБухгалтерии.Хозрасчетный.Обороты(&ДатаНач, &ДатаКон, , Счет В ИЕРАРХИИ (&СчетДоходов), , Организация = &Организация, КорСчет В ИЕРАРХИИ (&Счет91_09), ) КАК РасходыМесяц
			     	  |
			     	  |ОБЪЕДИНИТЬ ВСЕ
			     	  |
			     	  |ВЫБРАТЬ
			     	  |	""ЕНВД"",
			     	  |	0,
			     	  |	0,
			     	  |	СУММА(ЕСТЬNULL(ДоходыГодЕНВД.СуммаОборотДт, 0)),
			     	  |	0
			     	  |ИЗ
			     	  |	РегистрБухгалтерии.Хозрасчетный.Обороты(&ДатаНачГода, &ДатаКон, Период, Счет В ИЕРАРХИИ (&СчетРасходовЕНВД), , Организация = &Организация, , ) КАК ДоходыГодЕНВД
			     	  |
			     	  |ОБЪЕДИНИТЬ ВСЕ
			     	  |
			     	  |ВЫБРАТЬ
			     	  |	""ЕНВД"",
			     	  |	0,
			     	  |	0,
			     	  |	0,
			     	  |	СУММА(ЕСТЬNULL(ДоходыМесяцЕНВД.СуммаОборотДт, 0))
			     	  |ИЗ
			     	  |	РегистрБухгалтерии.Хозрасчетный.Обороты(&ДатаНач, &ДатаКон, Период, Счет В ИЕРАРХИИ (&СчетРасходовЕНВД), , Организация = &Организация, , ) КАК ДоходыМесяцЕНВД
			     	  |ИТОГИ ПО
			     	  |	ОБЩИЕ,
			     	  |	Показатель";
			
			Если Не ВидУчета = "БУ" Тогда
				Текст = СтрЗаменить(Текст, "Организация = &Организация, , )", "ВидУчета = &ВидУчета И Организация = &Организация, , )");
				Текст = СтрЗаменить(Текст, "Организация = &Организация)", "ВидУчетаДт = &ВидУчета И ВидУчетаКт = &ВидУчета И Организация = &Организация)");
				Текст = СтрЗаменить(Текст, "Организация = &Организация,", "ВидУчета = &ВидУчета И Организация = &Организация,");
				Текст = СтрЗаменить(Текст, "Хозрасчетный", "Налоговый");
			КонецЕсли;
			
			Если ВидУчета = "НУ" И ВидЗапроса = "ОсновнаяДеятельность" Тогда
				Текст = СтрЗаменить(Текст, "Субконто1", "Счет");
				Текст = СтрЗаменить(Текст, "СубконтоДт1", "СчетДт");
			КонецЕсли;
			
			Если ВидЗапроса = "НеОсновнаяДеятельность" И Не ЭтоУПП Тогда
				Текст = СтрЗаменить(Текст, "Организация = &Организация, , )", "Субконто1.ОтнесениеРасходовКДеятельностиЕНВД = &ННП И Организация = &Организация, , )");
				Текст = СтрЗаменить(Текст, "Организация = &Организация)", "СубконтоДт1.ОтнесениеРасходовКДеятельностиЕНВД = &ННП И Организация = &Организация)");
				Текст = СтрЗаменить(Текст, "(&СчетДоходов), , Организация = &Организация", "(&СчетДоходов), , Организация = &Организация И Субконто1.ОтнесениеРасходовКДеятельностиЕНВД = &ННП");
			КонецЕсли;
			
			Если ВидЗапроса = "Распределяемые расходы" И Не ЭтоУПП Тогда
