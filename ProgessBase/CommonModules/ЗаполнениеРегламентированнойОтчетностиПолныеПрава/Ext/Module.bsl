Процедура ВывестиРезультатЗапросаВДеревоПоказателей(УзелТаблицы, РезультатЗапроса)
	
	КолонкиРезультата = РезультатЗапроса.Колонки;
	
	Выборка = РезультатЗапроса.Выбрать();
	Пока Выборка.Следующий() Цикл 
		Для Каждого Колонка из КолонкиРезультата Цикл
			Если Лев(Колонка.Имя,3) = "П00" Тогда
				ОпределитьСтрокуВДеревеДанныхДляЗаполнения(УзелТаблицы, Колонка.Имя, ?(Не ЗначениеЗаполнено(Выборка[Колонка.Имя]),0,Выборка[Колонка.Имя]));
			КонецЕсли;
		КонецЦикла; 
	КонецЦикла; 

КонецПроцедуры

Процедура ВывестиСтрокуТаблицыДанныхВДеревоПоказателей(УзелРаздела, ТаблицаДанных)
	
	КолонкиРезультата = ТаблицаДанных.Колонки;
	
	СтрокаДанных = ТаблицаДанных[0];
	Для Каждого Колонка из КолонкиРезультата Цикл
		Если Лев(Колонка.Имя,3) = "П00" Тогда
			ОпределитьСтрокуВДеревеДанныхДляЗаполнения(УзелРаздела, Колонка.Имя, ?(Не ЗначениеЗаполнено(СтрокаДанных[Колонка.Имя]),0,СтрокаДанных[Колонка.Имя]));
		КонецЕсли;
	КонецЦикла; 
	
КонецПроцедуры

Процедура ВывестиТаблицуДанныхВДеревоПоказателей(УзелРаздела, ТаблицаДанных, ИмяМногострочнойЧасти = "")

	КолонкиРезультата = ТаблицаДанных.Колонки;
	
	Для каждого СтрокаДанных Из ТаблицаДанных Цикл
		УзелТаблицы = ОпределитьСтрокуВДеревеДанныхДляЗаполнения(УзелРаздела, ИмяМногострочнойЧасти);
		ОпределитьСтрокуВДеревеДанныхДляЗаполнения(УзелТаблицы, ИмяМногострочнойЧасти + "01", ТаблицаДанных.Индекс(СтрокаДанных) + 1);
		Для Каждого Колонка из КолонкиРезультата Цикл
			Если Лев(Колонка.Имя,3) = "П00" Тогда
				Значение = СтрокаДанных[Колонка.Имя];
				Если Значение = Null Или Значение = Неопределено Тогда
					Значение = 0
				КонецЕсли;
				ОпределитьСтрокуВДеревеДанныхДляЗаполнения(УзелТаблицы, Колонка.Имя, Значение);
			КонецЕсли;
		КонецЦикла; 
	КонецЦикла;
	
КонецПроцедуры

Функция ОпределитьСтрокуВДеревеДанныхДляЗаполнения(Узел, Имя, Значение = Неопределено)
	
	НовСтр = Узел.Строки.Добавить();
	НовСтр.Имя = Имя;
	НовСтр.Значение = Значение;
	Возврат НовСтр;
	
КонецФункции

// преобразование в массив рассчитывает на то, что дерево показателей является двухуровневым: 
// строки разделов содержат только строки показателей
Функция МассивЗначенийДерева(ДеревоПоказателей)

	МассивСтрокДерева = Новый Массив;
	Для Каждого Строка Из ДеревоПоказателей.Строки Цикл
		
		ТаблицаПоказателей = Новый ТаблицаЗначений;
		ТаблицаПоказателей.Колонки.Добавить("Имя");
		ТаблицаПоказателей.Колонки.Добавить("Значение");
		
		Для каждого СтрокаПоказателя Из Строка.Строки Цикл
			ЗаполнитьЗначенияСвойств(ТаблицаПоказателей.Добавить(), СтрокаПоказателя);
		КонецЦикла;
		
		МассивСтрокДерева.Добавить(Новый Структура("ИмяРаздела,ТаблицаПоказателей", Строка.Имя, ТаблицаПоказателей))
		
	КонецЦикла;

	Возврат МассивСтрокДерева
	
КонецФункции 

