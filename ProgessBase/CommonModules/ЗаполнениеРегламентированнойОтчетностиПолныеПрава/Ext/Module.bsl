		СтрокаВзносов = ТаблицаВзносов.Скопировать(СтруктураПоиска);
		СтрокаЧисленности = ТаблицаЧисленности.Скопировать(СтруктураПоиска);
		
		ОпределитьСтрокуВДеревеДанныхДляЗаполнения(УзелРаздел24, "П000240001001", СтрокаРезультата.Основание);
		
		Если СтрокаДоходов.Количество() = 0 И СтрокаВзносов.Количество() = 0 И СтрокаЧисленности.Количество() = 0 Тогда // по текущему основанию нет данных
			Если КоличествоОснований > 1 И ВсеОснования.Индекс(СтрокаРезультата) = ИндексПоследнейСтроки Тогда
				ДеревоПоказателей.Строки.Удалить(УзелРаздел24)
			КонецЕсли;
			Продолжить;
		КонецЕсли;
		
		Если СтрокаДоходов.Количество() <> 0 Тогда
			ВывестиСтрокуТаблицыДанныхВДеревоПоказателей(УзелРаздел24, СтрокаДоходов)
		КонецЕсли;
		Если СтрокаВзносов.Количество() <> 0 Тогда
			ВывестиСтрокуТаблицыДанныхВДеревоПоказателей(УзелРаздел24, СтрокаВзносов)
		КонецЕсли;
		Если СтрокаЧисленности.Количество() <> 0 Тогда
			ВывестиСтрокуТаблицыДанныхВДеревоПоказателей(УзелРаздел24, СтрокаЧисленности)
		КонецЕсли;
		
		// если применяемых в орг. оснований больше 1, тогда создадим еще одну ветку для раздела 2.4
		Если ВсеОснования.Индекс(СтрокаРезультата) < ИндексПоследнейСтроки Тогда  
