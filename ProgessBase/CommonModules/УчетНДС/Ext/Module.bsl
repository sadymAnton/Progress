		Табл.Колонки.Добавить("ДокументОснованиеДата");
		
		Табл.Колонки.Добавить("НомерОтделения");
		Табл.Колонки.Добавить("КППГрузополучателя");
		
	КонецЕслИ;	
		НС = Табл.Добавить();
