			МассПустыхДоков.Добавить(Документы[ИмяМета].ПустаяСсылка());
			
			Если ИмяМета = "СчетФактураПолученный" Тогда
				ПРГ_ДобавитьВСтроку_ПРГ_ПолучитьТекстЗапросаПоИспСЧФКнигаПокупок(ТаблДляЗапроса, Тип("ДокументСсылка."+ТекМета.Имя),
				ТекМета.Имя,"НДСЗаписиКнигиПокупок.СчетФактура",
				"СчетФактураПолученный.Контрагент",
				"""""","ДатаВремя(1,1,1)","СчетФактураПолученный.ДокументОснование.Дата","Ложь");
			ИначеЕсли ИмяМета = "ВозвратТоваровОтПокупателя" Тогда
				ПРГ_ДобавитьВСтроку_ПРГ_ПолучитьТекстЗапросаПоИспСЧФКнигаПокупок(ТаблДляЗапроса, Тип("ДокументСсылка."+ТекМета.Имя),
				ТекМета.Имя,"НДСЗаписиКнигиПокупок.СчетФактура",
				"ВозвратТоваровОтПокупателя.Контрагент",
				"""""","ДатаВремя(1,1,1)","ДатаВремя(1,1,1)","ВозвратТоваровОтПокупателя.ПокупателемВыставляетсяСчетФактураНаВозврат");
			ИначеЕсли ИмяМета = "СчетФактураВыданный" Тогда
				ПРГ_ДобавитьВСтроку_ПРГ_ПолучитьТекстЗапросаПоИспСЧФКнигаПокупок(ТаблДляЗапроса,Тип("ДокументСсылка."+ТекМета.Имя),
				ТекМета.Имя,"НДСЗаписиКнигиПокупок.СчетФактура",
				"СчетФактураВыданный.Контрагент",
				"""""","ДатаВремя(1,1,1)","СчетФактураВыданный.ДокументОснование.Дата","Ложь");
			Иначе
				ПРГ_ДобавитьВСтроку_ПРГ_ПолучитьТекстЗапросаПоИспСЧФКнигаПокупок(ТаблДляЗапроса,Тип("ДокументСсылка."+ТекМета.Имя),
