Функция ПолучитьИндексПодразделения(ТекСчетФактура)
	
	ИндексПодразделения = "";	
	Если ТипЗнч(ТекСчетФактура) = Тип("ДокументСсылка.СчетФактураВыданный") Тогда		
		//начало изменений Ожиганов 18.11.2015 б/н оптимизация книги покупок
		//ИндексПодразделения = ПолучитьРеализацию(ТекСчетФактура.ДокументОснование);		
		АРекв =  ПРГ_ДопФункцииКлиентСервер.ПолучитьСтруктРевизитаЗаказаСложНПоля(ТекСчетФактура,Новый Структура("ДокументОснование"));
		Если АРекв = Неопределено Тогда
			возврат "";
		Иначе
			ИндексПодразделения = ПолучитьРеализацию(АРекв.ДокументОснование);
		КонецЕсли;	
		//конец изменений 
	ИначеЕсли ТипЗнч(ТекСчетФактура) = Тип("ДокументСсылка.РеализацияТоваровУслуг") Тогда		
		ИндексПодразделения = ТекСчетФактура.КП_Отделение.НомерОтделения;		
	ИначеЕсли ТипЗнч(ТекСчетФактура) = Тип("ДокументСсылка.КорректировкаРеализации") Тогда		
		ИндексПодразделения = ПолучитьРеализацию(ТекСчетФактура.ДокументРеализации);		      		
	КонецЕсли;	
	Возврат ИндексПодразделения;
	
КонецФункции

Функция ПолучитьРеализацию(ТекДокОснование) Экспорт
	//начало изменений Ожиганов 18.11.2015 б/н оптимизация книги покупок
	Перем ЗапросКреал,ЗапросКДокРеал;
	//конец изменений 
	 
	
	Если ТипЗнч(ТекДокОснование) = Тип("ДокументСсылка.КорректировкаРеализации") Тогда 
		СледДокОснование = ТекДокОснование;	
		Пока ТипЗнч(СледДокОснование) <> Тип("ДокументСсылка.РеализацияТоваровУслуг") Цикл		
			//начало изменений Ожиганов 18.11.2015 б/н оптимизация книги покупок
			//подстрахуемся  если  корректировка не введена на основании реализации
			Если ТипЗнч(СледДокОснование) = Тип("ДокументСсылка.КорректировкаРеализации") Тогда
				Если ЗапросКДокРеал = Неопределено Тогда
					ЗапросКДокРеал = Новый Запрос("ВЫБРАТЬ
					                              |	КорректировкаРеализации.ДокументРеализации
					                              |ИЗ
					                              |	Документ.КорректировкаРеализации КАК КорректировкаРеализации
					                              |ГДЕ
					                              |	КорректировкаРеализации.Ссылка = &Ссылка");
				КонецЕсли;	
				//конец изменений 
				ЗапросКДокРеал.УстановитьПараметр("Ссылка",СледДокОснование);
				Выборка = ЗапросКДокРеал.Выполнить().Выбрать();
				Если Выборка.Следующий() Тогда
					СледДокОснование = Выборка.ДокументРеализации;
				Иначе // подстрахуемся
					возврат "";
				КонецЕсли;	
				//СледДокОснование = СледДокОснование.ДокументРеализации;
			Иначе
				// поскольку КП_Отделение реквизит реализации то в других документах он не используется
				возврат "";
			КонецЕсли;	
			//конец изменений 
		КонецЦикла;	
		//начало изменений Ожиганов 26.11.2015 б/н оптимизация книги продаж
		ЗапросКреал	 = Новый Запрос("ВЫБРАТЬ
		                              |	ЕСТЬNULL(Док.КП_Отделение.НомерОтделения, """") КАК НомерОтделения
		                              |ИЗ
		                              |	Документ.РеализацияТоваровУслуг КАК Док
		                              |ГДЕ
		                              |	Док.Ссылка = &Ссылка");
		ЗапросКреал.УстановитьПараметр("Ссылка",СледДокОснование);							  
		Выборка = ЗапросКреал.Выполнить().Выбрать();
		Если Выборка.Следующий() Тогда
			возврат Выборка.НомерОтделения;
		Иначе
			возврат "";
		КонецЕслИ;	
        //Возврат СледДокОснование.КП_Отделение.НомерОтделения;
		//конец изменений 
	 ИначеЕсли ТипЗнч(ТекДокОснование) = Тип("ДокументСсылка.РеализацияТоваровУслуг") Тогда
		 //начало изменений Ожиганов 18.11.2015 б/н оптимизация книги покупок
		ЗапросКреал	 = Новый Запрос("ВЫБРАТЬ
		                              |	ЕСТЬNULL(Док.КП_Отделение.НомерОтделения, """") КАК НомерОтделения
		                              |ИЗ
		                              |	Документ.РеализацияТоваровУслуг КАК Док
		                              |ГДЕ
		                              |	Док.Ссылка = &Ссылка");
									  
		ЗапросКреал.УстановитьПараметр("Ссылка",ТекДокОснование);
		Выборка = ЗапросКреал.Выполнить().Выбрать();
		Если Выборка.Следующий() Тогда
			возврат Выборка.НомерОтделения;
		Иначе
			возврат "";
		КонецЕслИ;	
		//Возврат ТекДокОснование.КП_Отделение.НомерОтделения;
		//конец изменений 
	Иначе
		Возврат "";
	КонецЕсли;
	
