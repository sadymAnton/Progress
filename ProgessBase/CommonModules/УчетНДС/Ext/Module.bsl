		НС.КППГрузополучателя		= КППГрузополучателя ;
		
		
КонецПроцедуры	

Процедура ПРГ_СформироватьДляЗапросаПоИспСЧФКнигаПродаж(ТипыИзмерения,ТаблДляЗапроса,МассПустыхСсылок = Неопределено)
	
	Если МассПустыхСсылок = Неопределено Тогда
		МассПустыхСсылок = Новый Массив;
	КонецЕсли;	
	
	Для Каждого ТекМета Из Метаданные.Документы Цикл
		Если ТипыИзмерения.Найти(Тип("ДокументСсылка."+ТекМета.Имя)) <> Неопределено Тогда
			ИмяМета = ТекМета.Имя;
			
			МассПустыхСсылок.Добавить(Документы[ИмяМета].ПустаяСсылка());
			
			Если ИмяМета = "СчетФактураПолученный" Тогда
				
				ПРГДобавитьВтаблДляИспСЧФКнигаПродаж(ТаблДляЗапроса, 
				Тип("ДокументСсылка."+ТекМета.Имя),					//типзнч
				ТекМета.Имя,                                        //имя таблицы
				"НДСЗаписиКнигиПродаж.СчетФактура",    		     	//счетфактура 
				"СчетФактураПолученный.ВидСчетаФактуры",        	//ВидСчетаФактуры
				"СчетФактураПолученный.ДатаВходящегоДокумента",     //ДатаВходящегоДокумента
				"СчетФактураПолученный.НомерВходящегоДокумента",    //НомерВходящегоДокумента
				"ДАТАВРЕМЯ(1, 1, 1)",                               //ДатаИсходногоДокумента
				"""""",                                               //НомерИсходногоДокумента
				"СчетФактураВыданный.Исправление",					//Исправление
				"ДАТАВРЕМЯ(1, 1, 1)",							    //документ основание дата
				"""""",                                               // НомерОтделения;
				"""""");	                                            // КППГрузополучателя ;
						
				
			ИначеЕсли ИмяМета = "СчетФактураВыданный" Тогда
				ПРГДобавитьВтаблДляИспСЧФКнигаПродаж(ТаблДляЗапроса, 
				Тип("ДокументСсылка."+ТекМета.Имя),				 ///типзнч
				ТекМета.Имя,                                     //имя таблицы
				"НДСЗаписиКнигиПродаж.СчетФактура",    		     //счетфактура 
				"СчетФактураВыданный.ВидСчетаФактуры",        	 //ВидСчетаФактуры
				"ДАТАВРЕМЯ(1, 1, 1)",							 //ДатаВходящегоДокумента
				"""""",    									     // НомерВходящегоДокумента
				"СчетФактураВыданный.ДатаИсходногоДокумента",	 //ДатаИсходногоДокумента
				"СчетФактураВыданный.НомерИсходногоДокумента",   //НомерИсходногоДокумента
				"СчетФактураВыданный.Исправление",				 //Исправление
				"СчетФактураВыданный.ДокументОснование.Дата",	//документ основание дата
				"""""",                                           // НомерОтделения;
				"""""");	                                        // КППГрузополучателя ;
				
			ИначеЕсли ИмяМета = "РеализацияТоваровУслуг" Тогда
				ПРГДобавитьВтаблДляИспСЧФКнигаПродаж(ТаблДляЗапроса, 
				Тип("ДокументСсылка."+ТекМета.Имя),				 ///типзнч
				ТекМета.Имя,                                     //имя таблицы
				"НДСЗаписиКнигиПродаж.СчетФактура",    		 //счетфактура 
				"NULL",  										//ВидСчетаФактуры
				"ДАТАВРЕМЯ(1, 1, 1)", 							//ДатаВходящегоДокумента
				"""""",    									    // НомерВходящегоДокумента
				"ДАТАВРЕМЯ(1, 1, 1)",						    //ДатаИсходногоДокумента
				"""""",   										//НомерИсходногоДокумента
				"Ложь",				 							//Исправление
				"ДАТАВРЕМЯ(1, 1, 1)",							//документ основание дата
				"ЕстьNull(РеализацияТоваровУслуг.КП_Отделение.НомерОтделения,"""")", // НомерОтделения;
				"ЕстьNull(РеализацияТоваровУслуг.Грузополучатель.КПП,"""")"); 		 // КППГрузополучателя ;
			Иначе	
				ПРГДобавитьВтаблДляИспСЧФКнигаПродаж(ТаблДляЗапроса, 
				Тип("ДокументСсылка."+ТекМета.Имя),				 ///типзнч
				ТекМета.Имя,                                     //имя таблицы
				"НДСЗаписиКнигиПродаж.СчетФактура",    		 //счетфактура 
				"NULL",        	 								//ВидСчетаФактуры
				"ДАТАВРЕМЯ(1, 1, 1)",    						//ДатаВходящегоДокумента
				"""""",    									     // НомерВходящегоДокумента
				"ДАТАВРЕМЯ(1, 1, 1)",							//ДатаИсходногоДокумента
				"""""",   										//НомерИсходногоДокумента
				"Ложь",				 							//Исправление
				"ДАТАВРЕМЯ(1, 1, 1)",							 //документ основание дата
				"""""",                                         // НомерОтделения;
				"""""");	                                    // КППГрузополучателя ;
				
			КонецЕслИ;	
			 //Сообщить(ТекМета.Имя);
		КонецЕсли;	
	КонецЦикла;	
	
КонецПроцедуры	

Функция ПРГ_СформТекстЗапросаДляПРГ_ПолучитьТекстЗапросаПоИспСЧФКнигаПродаж(ЗапросПоТипам,ТаблДляЗапроса,ИмяСчф,ТекстДопУсловия, ИмяТабл,ЗаписьДопЛиста) 
	Выборка = ЗапросПоТипам.Выполнить().Выбрать();
	ТекстДляЗапроса = "";
	Если Выборка.Количество() > 0 Тогда
		Пока Выборка.Следующий() Цикл
			
			НС = ТаблДляЗапроса.Найти(Выборка.ТипЗначения,"ТипЗнч");
			Если НС = Неопределено тогда
				вызватьИсключение "не обнаружен тип "+Выборка.ТипЗначения;
			Иначе
				ТекстДляЗапроса = ТекстДляЗапроса
				+"
				|"+?(ПустаяСтрока(ТекстДляЗапроса),"Выбрать","
				|ОБЪЕДИНИТЬ ВСЕ
				|Выбрать")+"
				| "+НС.ИмяТабл+".Ссылка  КАК  СчетФактура,
				| "+НС.ИмяТабл + ".Дата Как СчетФактураДата,
				| "+НС.ИмяТабл + ".Номер Как СчетФактураНомер,
				| "+НС.ВидСчетаФактуры+ " Как СчетФактураВидСчетаФактуры,
				| "+НС.НомерВходящегоДокумента+ " Как СчетФактураНомерВходящегоДокумента,
				| "+НС.ДатаВходящегоДокумента+ " Как СчетФактураДатаВходящегоДокумента,
				| "+НС.НомерИсходногоДокумента+ " Как СчетФактураНомерИсходногоДокумента,
				| "+НС.ДатаИсходногоДокумента+ " Как СчетФактураДатаИсходногоДокумента,
				| "+НС.Исправление+ " Как СчетФактураИсправление,
				| "+НС.НомерОтделения + " Как НомерОтделения,
				| "+НС.КППГрузополучателя + " Как КППГрузополучателя,
				| "+НС.ДокументОснованиеДата+ " Как СчетФактураДокументОснованиеДата
				| из 
				|	РегистрНакопления.НДСЗаписиКнигиПродаж КАК НДСЗаписиКнигиПродаж
				|	 Внутреннее СОЕДИНЕНИЕ Документ."+НС.ИмяТабл+" как "+НС.ИмяТабл+"
				|	 ПО НДСЗаписиКнигиПродаж."+ИмяСчф+" = "+НС.ИмяТабл+".Ссылка
				|"+?(ЗаписьДопЛиста," И НДСЗаписиКнигиПродаж.ЗаписьДополнительногоЛиста ","")+"
		        |	ГДЕ
		        |		НДСЗаписиКнигиПродаж.Период МЕЖДУ &НачалоПериода И &КонецПериода
				| " + ТекстДопУсловия+"
				|";
				
			КонецЕслИ;	
		КонецЦикла;	
	Иначе
		
				ТекстДляЗапроса = 
				"
				|Выбрать первые 0
				|  NULL КАК  СчетФактура,
				|  ДатаВремя(1,1,1) Как СчетФактураДата,
				| """" Как СчетФактураНомер,
				| NULL Как СчетФактураВидСчетаФактуры,
				| """" Как СчетФактураНомерВходящегоДокумента,
				| ДатаВремя(1,1,1) Как СчетФактураДатаВходящегоДокумента,
				| """" Как СчетФактураНомерИсходногоДокумента,
				| ДатаВремя(1,1,1) Как СчетФактураДатаИсходногоДокумента,
				| Ложь Как СчетФактураИсправление,
				| """" Как НомерОтделения,
				| """" Как КППГрузополучателя,
				| ДатаВремя(1,1,1) Как СчетФактураДокументОснованиеДата
				|";
		
		
	КонецЕсли;	
			
			
	возврат  "ВЫБРАТЬ
	|ПРГ_ИнфаПОСЧФ.СчетФактура,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДата,
	|ПРГ_ИнфаПОСЧФ.СчетФактураНомер,
	|ПРГ_ИнфаПОСЧФ.СчетФактураВидСчетаФактуры,
	|ПРГ_ИнфаПОСЧФ.СчетФактураНомерВходящегоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДатаВходящегоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДатаИсходногоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураНомерИсходногоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураИсправление,
	|ПРГ_ИнфаПОСЧФ.НомерОтделения,
	|ПРГ_ИнфаПОСЧФ.КППГрузополучателя,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДокументОснованиеДата
	
	|ПОМЕСТИТЬ  "+ ИмяТабл+ " //ПРГ_ИнфаПОСчф
	|ИЗ
	|(
	| "+ ТекстДляЗапроса+"
	|) КАК ПРГ_ИнфаПОСЧФ
	|СГРУППИРОВАТЬ ПО
	|ПРГ_ИнфаПОСЧФ.СчетФактура,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДата,
	|ПРГ_ИнфаПОСЧФ.СчетФактураНомер,
	|ПРГ_ИнфаПОСЧФ.СчетФактураВидСчетаФактуры,
	|ПРГ_ИнфаПОСЧФ.СчетФактураНомерВходящегоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДатаВходящегоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДатаИсходногоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураНомерИсходногоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураИсправление,
	|ПРГ_ИнфаПОСЧФ.НомерОтделения,
	|ПРГ_ИнфаПОСЧФ.КППГрузополучателя,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДокументОснованиеДата
	|
	| //ИНДЕКСИРОВАТЬ ПО
	|//	СчетФактура";

	
	
КонецФункции	


Функция ПРГ_ПолучитьТекстЗапросаПоИспСЧФКнигаПродаж(НачалоПериода,КонецПериода,ЗаписДопЛиста=Ложь)
	
	ТаблДляЗапроса = Неопределено;
	
	МетаИзм = Метаданные.РегистрыНакопления.НДСЗаписиКнигиПродаж.Измерения.СчетФактура;
	ТипыИзмерения  =  МетаИзм.Тип.Типы();
	ПРГ_СформироватьДляЗапросаПоИспСЧФКнигаПродаж(ТипыИзмерения,ТаблДляЗапроса) ;
	ТаблДляЗапроса.Индексы.Добавить("ТипЗнч");
	
	ТаблДляЗапросаИсправление = Неопределено;
	
	МетаИзм = Метаданные.РегистрыНакопления.НДСЗаписиКнигиПродаж.Измерения.ИсправленныйСчетФактура;
	ТипыИзмерения  =  МетаИзм.Тип.Типы();
	МассПустыхСсылок = Неопределено;
	ПРГ_СформироватьДляЗапросаПоИспСЧФКнигаПродаж(ТипыИзмерения,ТаблДляЗапросаИсправление,МассПустыхСсылок) ;
	ТаблДляЗапросаИсправление.Индексы.Добавить("ТипЗнч");
	
	
	ЗапросПоТипам = Новый Запрос("ВЫБРАТЬ
	                             |	ТИПЗНАЧЕНИЯ(НДСЗаписиКнигиПродаж.СчетФактура) КАК ТипЗначения
	                             |ИЗ
	                             |	РегистрНакопления.НДСЗаписиКнигиПродаж КАК НДСЗаписиКнигиПродаж
								 |	ГДЕ
	        					 |		НДСЗаписиКнигиПродаж.Период МЕЖДУ &НачалоПериода И &КонецПериода
								 |  и не ТИПЗНАЧЕНИЯ(НДСЗаписиКнигиПродаж.СчетФактура) = ТипЗначения(Null) 
								 |"+?(ЗаписДопЛиста," И НДСЗаписиКнигиПродаж.ЗаписьДополнительногоЛиста ","")+"
	                             |СГРУППИРОВАТЬ ПО
	                             |	ТИПЗНАЧЕНИЯ(НДСЗаписиКнигиПродаж.СчетФактура)");
								 
//начало изменений Ожиганов 15.01.2016 б/н ошибка с датами
	ЗапросПоТипам.УстановитьПараметр("НачалоПериода",НачалоДня(НачалоПериода));
	ЗапросПоТипам.УстановитьПараметр("КонецПериода",КонецДня(КонецПериода));
//конец изменений 	
	
	ЗапросПоТипамКИсправлениям = Новый Запрос("ВЫБРАТЬ
	                                          |	ТИПЗНАЧЕНИЯ(НДСЗаписиКнигиПродаж.ИсправленныйСчетФактура) КАК ТипЗначения
	                                          |ИЗ
	                                          |	РегистрНакопления.НДСЗаписиКнигиПродаж КАК НДСЗаписиКнигиПродаж
	                                          |ГДЕ
	                                          |	НДСЗаписиКнигиПродаж.Период МЕЖДУ &НачалоПериода И &КонецПериода
											  | И НЕ ТИПЗНАЧЕНИЯ(НДСЗаписиКнигиПродаж.ИсправленныйСчетФактура) = ТипЗначения(NULL)
	                                          |	И НЕ НДСЗаписиКнигиПродаж.ИсправленныйСчетФактура В (&ПустыеДокументыИсправления)
											  |"+?(ЗаписДопЛиста," И НДСЗаписиКнигиПродаж.ЗаписьДополнительногоЛиста ","")+"
	                                          |
	                                          |СГРУППИРОВАТЬ ПО
	                                          |	ТИПЗНАЧЕНИЯ(НДСЗаписиКнигиПродаж.ИсправленныйСчетФактура)");
											  
//начало изменений Ожиганов 15.01.2016 б/н ошибка с датами 											  
	ЗапросПоТипамКИсправлениям.УстановитьПараметр("НачалоПериода",НачалоДня(НачалоПериода));
	ЗапросПоТипамКИсправлениям.УстановитьПараметр("КонецПериода",КонецДня(КонецПериода));
//конец изменений 	
	ЗапросПоТипамКИсправлениям.УстановитьПараметр("ПустыеДокументыИсправления",МассПустыхСсылок);
	
	возврат  "
	//ПРГ_СформТекстЗапросаДляПРГ_ПолучитьТекстЗапросаПоИспСЧФКнигаПродаж(ЗапросПоТипам,ТаблДляЗапроса,ИмяСчф,ТекстДопУсловия, ИмяТабл)
	|"+ПРГ_СформТекстЗапросаДляПРГ_ПолучитьТекстЗапросаПоИспСЧФКнигаПродаж(ЗапросПоТипам,ТаблДляЗапроса,"СчетФактура","","ПРГ_ИнфаПОСчф",ЗаписДопЛиста)+"
	|;
	| "+ПРГ_СформТекстЗапросаДляПРГ_ПолучитьТекстЗапросаПоИспСЧФКнигаПродаж(ЗапросПоТипамКИсправлениям,ТаблДляЗапроса,"ИсправленныйСчетФактура","И НЕ НДСЗаписиКнигиПродаж.ИсправленныйСчетФактура В (&ПустыеДокументыИсправления)","ПРГ_ИнфаПОИспрСчф",ЗаписДопЛиста) + "
	|";
	
КонецФункции	
