Процедура ВывестиТаблицуСтавкиНДС_Подвал(Макет, ТабДокумент, ВыборкаСтрокТовары)

	//Выводим ставки НДС
	ОбластьЗаголовокНДС	 = Макет.ПолучитьОбласть("ЗаголовокНДС");
	ОбластьДанныеНДС	 = Макет.ПолучитьОбласть("ДанныеНДС");
	ОбластьИтогНДС		 = Макет.ПолучитьОбласть("ИтогиНДС");
	
	РезНДС = ВыборкаСтрокТовары.Скопировать();
	
	РезНДС.Свернуть("СтавкаНДС", "СуммаСНДС, СуммаБезНДС, СуммаНДС");
	Если НЕ РезНДС.Количество() = 0 Тогда
		ТабДокумент.Вывести(ОбластьЗаголовокНДС); 
		Для Каждого ТекНДС Из РезНДС Цикл
			ОбластьДанныеНДС.Параметры.СтавкаНДС = СокрЛП(ТекНДС.СтавкаНДС);
			ОбластьДанныеНДС.Параметры.СуммаНДС = Формат(ТекНДС.СуммаНДС, "ЧДЦ=2; ЧГ=0");
			ОбластьДанныеНДС.Параметры.СуммаБезНДС = Формат(ТекНДС.СуммаБезНДС, "ЧДЦ=2; ЧГ=0");
			ОбластьДанныеНДС.Параметры.СуммаСНДС = Формат(ТекНДС.СуммаСНДС, "ЧДЦ=2; ЧГ=0");
			ТабДокумент.Вывести(ОбластьДанныеНДС); 
		КонецЦикла;
		РезНДС.Свернуть(, "СуммаНДС, СуммаБезНДС, СуммаСНДС");
		ОбластьИтогНДС.Параметры.СуммаНДС = Формат(РезНДС[0].СуммаНДС, "ЧДЦ=2; ЧГ=0");
