				?(ТекМета.Реквизиты.Найти("Контрагент") = Неопределено,"Значение(Справочник.Контрагенты.ПустаяСсылка)",ТекМета.Имя+".Контрагент"),
				?(ТекМета.Реквизиты.Найти("НомерВходящегоДокумента") = Неопределено,"""""",ТекМета.Имя+".НомерВходящегоДокумента"),
				?(ТекМета.Реквизиты.Найти("ДатаВходящегоДокумента") = Неопределено ,"ДатаВремя(1,1,1)",ТекМета.Имя+".ДатаВходящегоДокумента"),
				"ДатаВремя(1,1,1)","Ложь");
				
			КонецЕслИ;	
			 //Сообщить(ТекМета.Имя);
		КонецЕсли;	
	КонецЦикла;	
	
	ТаблДляЗапроса.Индексы.Добавить("ТипЗнч");
	
КонецПроцедуры	

Функция ПРГ_СформТекстЗапросаДляПолучитьТекстЗапросаПоИспСЧФКнигаПокупок(ЗапросПоТипам,ТаблДляЗапроса,ИмяРекв,ИмяТабл,ЗаписДопЛиста)
	
	Выборка = ЗапросПоТипам.Выполнить().Выбрать();
	ТекстДляЗапроса = "";
	Если Выборка.Количество() Тогда
		Пока Выборка.Следующий() Цикл
			
			НС = ТаблДляЗапроса.Найти(Выборка.ТипЗначения,"ТипЗнч");
			Если НС = Неопределено тогда
				вызватьИсключение "не обнаружен тип "+Выборка.ТипЗначения;
			Иначе
				ТекстДляЗапроса = ТекстДляЗапроса
				+"
				|"+?(ПустаяСтрока(ТекстДляЗапроса),"Выбрать","
				|ОБЪЕДИНИТЬ ВСЕ
				|Выбрать")+"
				| "+НС.ИмяТабл+".Ссылка КАК  СчетФактура,
				| "+НС.ИмяТабл + ".Дата Как СчетФактураДата,
				| "+НС.ИмяТабл + ".Номер Как СчетФактураНомер,
				| "+НС.Контрагент+ " Как СчетФактураКонтрагент,
				| "+НС.НомерВходящегоДокумента+ " Как СчетФактураНомерВходящегоДокумента,
				| "+НС.ДатаВходящегоДокумента+ " Как СчетФактураДатаВходящегоДокумента,
				| "+НС.ДокументОснованиеДата+ " Как СчетФактураДокументОснованиеДата,
				| "+НС.ПокупателемВыставляетсяСчетФактураНаВозврат+ " Как ПокупателемВыставляетсяСчетФактураНаВозврат
				| из 
				|	РегистрНакопления.НДСЗаписиКнигиПокупок КАК НДСЗаписиКнигиПокупок
				|	 Внутреннее СОЕДИНЕНИЕ Документ."+НС.ИмяТабл+" как "+НС.ИмяТабл+"
				|	 ПО НДСЗаписиКнигиПокупок."+ИмяРекв+" = "+НС.ИмяТабл+".Ссылка
		        |	ГДЕ
		        |		НДСЗаписиКнигиПокупок.Период МЕЖДУ &НачалоПериода И &КонецПериода
				|"+?(ЗаписДопЛиста," И НДСЗаписиКнигиПокупок.ЗаписьДополнительногоЛиста","")+"
				|";
				
			КонецЕслИ;	
			
			
		КонецЦикла;	
	Иначе
				ТекстДляЗапроса = "ВЫБРАТЬ ПЕРВЫЕ 0
				 |	NULL КАК СчетФактура,
				 |	NULL КАК СчетФактураДата,
				 |	NULL КАК СчетФактураНомер,
				 |	NULL КАК СчетФактураКонтрагент,
				 |	NULL КАК СчетФактураНомерВходящегоДокумента,
				 |	NULL КАК СчетФактураДатаВходящегоДокумента,
				 |	NULL КАК СчетФактураДокументОснованиеДата,
				 |	Ложь КАК ПокупателемВыставляетсяСчетФактураНаВозврат";
		
	КонецЕсли;	
	
	
	возврат  "ВЫБРАТЬ
	|ПРГ_ИнфаПОСЧФ.СчетФактура,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДата,
	|ПРГ_ИнфаПОСЧФ.СчетФактураНомер,
	|ПРГ_ИнфаПОСЧФ.СчетФактураКонтрагент,
	|ПРГ_ИнфаПОСЧФ.СчетФактураНомерВходящегоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДатаВходящегоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДокументОснованиеДата,
	|ПРГ_ИнфаПОСЧФ.ПокупателемВыставляетсяСчетФактураНаВозврат
	|ПОМЕСТИТЬ "+ИмяТабл+" //ПРГ_ИнфаПОСчф
	|ИЗ
	|(
	| "+ ТекстДляЗапроса+"
	|) КАК ПРГ_ИнфаПОСЧФ
	|СГРУППИРОВАТЬ ПО
	|ПРГ_ИнфаПОСЧФ.ПокупателемВыставляетсяСчетФактураНаВозврат,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДокументОснованиеДата,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДатаВходящегоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураНомерВходящегоДокумента,
	|ПРГ_ИнфаПОСЧФ.СчетФактураКонтрагент,
	|ПРГ_ИнфаПОСЧФ.СчетФактураНомер,
	|ПРГ_ИнфаПОСЧФ.СчетФактураДата,
	|ПРГ_ИнфаПОСЧФ.СчетФактура
	|
	|//ИНДЕКСИРОВАТЬ ПО
	|//	СчетФактура";
	
	
КонецФункции	


Функция ПРГ_ПолучитьТекстЗапросаПоИспСЧФКнигаПокупок(НачалоПериода,КонецПериода,ЗаписДопЛиста=Ложь)
	
	ТаблДляЗапроса = Неопределено;
	
