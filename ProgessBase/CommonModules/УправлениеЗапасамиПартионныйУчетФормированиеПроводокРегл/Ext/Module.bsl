Процедура ПРГВыполнитьДвиженияПоОтклВСтоимостиБУХ(СписаноИз, СтрокаДокумента, СтруктураПараметров, Движение, ТипЗаписи = "Прямая", ПриходоватьВ = Неопределено)
	
	Если НЕ СтрокаДокумента.ОтражатьВБухгалтерскомУчете Тогда
		Возврат;
	КонецЕсли;
	
	ИмяРегистра = "Хозрасчетный";
	
	Количество = Движение.Количество;
	Сумма      = Движение.Стоимость;
	
	Если Количество = 0 и Сумма = 0 Тогда
		возврат;
	КонецЕсли;	
	
	Если Сумма >= 0 Тогда 
		
		СчетКт        = Движение.СчетУчета;
		СчетДт        = СтрокаДокумента.ПРГСчетРасходов;
		
		Знак 		  = 1;
		
	Иначе 
		
		СчетДт        = Движение.СчетУчета;
		СчетКт        = СтрокаДокумента.ПРГСчетДоходов;
		
		Знак 		  = -1;
		
	КонецЕсли;	
	
	 Движение.КорСчет	   = СтрокаДокумента.КорсчетБУ;
	 Движение.КорСубконто1 = СтрокаДокумента.КорСубконтоБУ1;
	 Движение.КорСубконто2 = СтрокаДокумента.КорСубконтоБУ2;
	 Движение.КорСубконто3 = СтрокаДокумента.КорСубконтоБУ3;
	
	
	Если Не ЗначениеЗаполнено(СчетКт) или  Не ЗначениеЗаполнено(СчетКт) Тогда
		возврат;
	КонецЕслИ;	
	
	
	Проводка 			   = СтруктураПараметров["Движения"+ИмяРегистра].Добавить();
	Проводка.Период        = СтруктураПараметров.Период;
	Проводка.Регистратор   = СтруктураПараметров.Регистратор;
	Проводка.Активность    = Истина;
	Проводка.Организация   = СтрокаДокумента.Организация;
	Проводка.Сумма 		   = Знак * Сумма;
	
	Проводка.СчетДт 	   = СчетДт;
	Проводка.СчетКт 	   = СчетКт;
	
	ПРГЗаполнитьСубконтоПоТоварамРегл(Проводка, СтрокаДокумента, Движение, СтруктураПараметров,Неопределено,СчетДт.ВидыСубконто,СчетКт.ВидыСубконто);
	
	Если Количество <> 0 Тогда 
		
		Если Проводка.СчетКт.Количественный Тогда
			//Количество в возврате и так "красное"
			Проводка.КоличествоКт = Количество;
		КонецЕсли;
		
		Если Проводка.СчетДт.Количественный Тогда
			//Количество в возврате и так "красное"
			Проводка.КоличествоДт = Количество;
		КонецЕсли;
	КонецЕсли;	
	
	//начало изменений 53363 формирование проводок при возрате тары по залоговой стоимости  
	Проводка.Содержание = "Отклонение в стоимости";
	СтруктураПараметров["ИзмененыДвижения"+ИмяРегистра] = Истина;
	//конец изменений 
	
КонецПроцедуры	


Процедура ПРГВыполнитьДвиженияПоОтклВСтоимостиНал(СписаноИз, СтрокаДокумента, СтруктураПараметров, Движение, ТипЗаписи)
	
	
	Если НЕ СтрокаДокумента.ОтражатьВНалоговомУчете Тогда
		Возврат;
	КонецЕсли;
	
	ИмяРегистра = "Налоговый";
	
	Количество = Движение.Количество;
	Сумма      = Движение.Стоимость;
	СуммаПР    = Движение.ПостояннаяРазница;
	СуммаВР    = Движение.ВременнаяРазница;
	
	
	Если Количество = 0 и Сумма = 0 и СуммаПР = 0 и СуммаВР = 0  Тогда
		возврат;
	КонецЕсли;	
	
	//начало изменений Ожиганов 10.03.2016 попытаемся через всю сумму получить такие же проводки по возврату как и в БУ
	СуммаДляЗнака = Сумма+СуммаПР+СуммаВР;
	//конец изменений 
	
	//начало изменений Ожиганов 10.03.2016 попытаемся через всю сумму получить такие же проводки по возврату как и в БУ
	//Если Сумма >= 0 Тогда 
	Если СуммаДляЗнака >= 0 Тогда 
	//конец изменений 	
		
		СчетКт        = Движение.СчетУчета;
		СчетДт        = СтрокаДокумента.ПРГСчетРасходов;
		
		Знак 		  = 1;
		
	Иначе 
		
		СчетДт        = Движение.СчетУчета;
		СчетКт        = СтрокаДокумента.ПРГСчетДоходов;
		
		Знак 		  = -1;
		
	КонецЕсли;	
	
	 Движение.КорСчет		 = СтрокаДокумента.КорСчетНУ;
	 Движение.КорСубконто1   = СтрокаДокумента.КорСубконтоНУ1;
	 Движение.КорСубконто2   = СтрокаДокумента.КорСубконтоНУ2;
	 Движение.КорСубконто3   = СтрокаДокумента.КорСубконтоНУ3;
	
	
	Если Не ЗначениеЗаполнено(СчетКт) или  Не ЗначениеЗаполнено(СчетКт) Тогда
		возврат;
	КонецЕслИ;	
	
	
	Для НомерПроводки = 1 По 3 Цикл
		
		
		Если НомерПроводки = 1 тогда
			Если Сумма = 0 и Количество = 0 Тогда
				продолжить;
			КонецЕсли;	
			
			ВидУчета = Перечисления.ВидыУчетаПоПБУ18.НУ;
			
		ИначеЕсли НомерПроводки = 2 тогда
			Если СуммаВР = 0 Тогда
				продолжить;
			КонецЕсли;	
			
			ВидУчета = Перечисления.ВидыУчетаПоПБУ18.ВР;
			
			//начало изменений Ожиганов 03.03.2016 исправление ошибки при возвратах
			Сумма    =  СуммаВР;
			//конец изменений 
			
		ИначеЕсли НомерПроводки = 3 тогда
			
			Если СуммаПР = 0 Тогда
				продолжить;
			КонецЕсли;	
			
			
			ВидУчета = Перечисления.ВидыУчетаПоПБУ18.ПР;
			//начало изменений Ожиганов 03.03.2016 исправление ошибки при возвратах
			Сумма = СуммаПР;
			//конец изменений 
			
		КонецЕсли;
			
	
		Проводка 			   = СтруктураПараметров["Движения"+ИмяРегистра].Добавить();
		Проводка.Период        = СтруктураПараметров.Период;
		Проводка.Регистратор   = СтруктураПараметров.Регистратор;
		Проводка.Активность    = Истина;
		Проводка.Организация   = СтрокаДокумента.Организация;
		Проводка.Сумма 		   = Знак * Сумма;
		
		Проводка.СчетДт 	   = СчетДт;
		Проводка.СчетКт 	   = СчетКт;
		
		Проводка.ВидУчетаДТ    = ВидУчета;
		Проводка.ВидУчетаКТ    = ВидУчета;
		
		
		ПРГЗаполнитьСубконтоПоТоварамРегл(Проводка, СтрокаДокумента, Движение, СтруктураПараметров,Неопределено,СчетДт.ВидыСубконто,СчетКт.ВидыСубконто);
		
		Если Количество <> 0 и НомерПроводки = 1  Тогда 
			
			Если Проводка.СчетКт.Количественный Тогда
				//Количество в возврате и так "красное"
				Проводка.КоличествоКт = Количество;
			КонецЕсли;
			
			Если Проводка.СчетДт.Количественный Тогда
				//Количество в возврате и так "красное"
				Проводка.КоличествоДт = Количество;
			КонецЕсли;
		КонецЕсли;		
	
	КонецЦикла;
	//начало изменений 53363 формирование проводок при возрате тары по залоговой стоимости  
	Проводка.Содержание = "Отклонение в стоимости";
	СтруктураПараметров["ИзмененыДвижения"+ИмяРегистра] = Истина;
	//конец изменений 
	
КонецПроцедуры	

Процедура ПРГВыполнитьДвиженияПоОтклВСтоимости(СписаноИз, СтрокаДокумента, СтруктураПараметров, Движение, ТипЗаписи = "Прямая", ПриходоватьВ = Неопределено) Экспорт
	
	Если СтруктураПараметров.Свойство("ИспользоватьРасширеннуюАналитику")
	  И СтруктураПараметров.ИспользоватьРасширеннуюАналитику
	  И УправлениеЗапасамиПартионныйУчет.ПолучитьПараметрУчетнойПолитикиПартионногоУчета("СистемаНалогообложения", "Бух", СтруктураПараметров) = Перечисления.СистемыНалогообложения.Упрощенная Тогда
		Возврат;
	КонецЕсли;	
	
	Если СтруктураПараметров.ФормироватьПроводкиПоСписаниюТМЦ Тогда
		ПРГВыполнитьДвиженияПоОтклВСтоимостиБУХ(СписаноИз, СтрокаДокумента, СтруктураПараметров, Движение, ТипЗаписи);
		
		ПРГВыполнитьДвиженияПоОтклВСтоимостиНал(СписаноИз, СтрокаДокумента, СтруктураПараметров, Движение, ТипЗаписи);
	КонецЕсли;	
	
КонецПроцедуры	
