<xs:schema xmlns:ns1="http://api.vetrf.ru/schema/cdm/base" xmlns:tns="http://api.vetrf.ru/schema/cdm/ikar" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://api.vetrf.ru/schema/cdm/ikar" attributeFormDefault="unqualified" elementFormDefault="qualified">
	<xs:import namespace="http://api.vetrf.ru/schema/cdm/base"/>
	<xs:element name="district" type="tns:District"/>
	<xs:element name="streetList" type="tns:StreetList"/>
	<xs:element name="localityGuid" type="ns1:UUID"/>
	<xs:element name="country" type="tns:Country"/>
	<xs:element name="regionGuid" type="ns1:UUID"/>
	<xs:element name="districtGuid" type="ns1:UUID"/>
	<xs:element name="countryGuid" type="ns1:UUID"/>
	<xs:element name="localityList" type="tns:LocalityList"/>
	<xs:element name="region" type="tns:Region"/>
	<xs:element name="regionList" type="tns:RegionList"/>
	<xs:element name="districtList" type="tns:DistrictList"/>
	<xs:element name="countryList" type="tns:CountryList"/>
	<xs:complexType name="Address">
		<xs:sequence>
			<xs:element name="country" type="tns:Country"/>
			<xs:element name="federalDistrict" type="tns:FederalDistrict" minOccurs="0"/>
			<xs:element name="region" type="tns:Region" minOccurs="0"/>
			<xs:element name="district" type="tns:District" minOccurs="0"/>
			<xs:element name="locality" type="tns:Locality" minOccurs="0"/>
			<xs:element name="subLocality" type="tns:Locality" minOccurs="0"/>
			<xs:element name="street" type="tns:Street" minOccurs="0"/>
			<xs:element name="house" type="ns1:String255" minOccurs="0"/>
			<xs:element name="building" type="ns1:String255" minOccurs="0"/>
			<xs:element name="room" type="ns1:String255" minOccurs="0"/>
			<xs:element name="postIndex" type="ns1:String255" minOccurs="0"/>
			<xs:element name="postBox" type="ns1:String255" minOccurs="0"/>
			<xs:element name="addressView" type="ns1:String255" minOccurs="0"/>
			<xs:element name="enAddressView" type="ns1:String255" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressObjectView">
		<xs:complexContent>
			<xs:extension base="ns1:GenericVersioningEntity">
				<xs:sequence>
					<xs:element name="name" type="ns1:String255" minOccurs="0"/>
					<xs:element name="englishName" type="ns1:String255" minOccurs="0"/>
					<xs:element name="view" type="ns1:String255" minOccurs="0"/>
					<xs:element name="regionCode" type="xs:string" minOccurs="0"/>
					<xs:element name="type" type="ns1:String255" minOccurs="0"/>
					<xs:element name="countryGuid" type="ns1:UUID" minOccurs="0"/>
					<xs:element name="hasStreets" type="xs:boolean" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="Code">
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Code3">
		<xs:restriction base="xs:string">
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Country">
		<xs:complexContent>
			<xs:extension base="ns1:GenericVersioningEntity">
				<xs:sequence>
					<xs:element name="name" type="ns1:String255" minOccurs="0"/>
					<xs:element name="fullName" type="ns1:String255" minOccurs="0"/>
					<xs:element name="englishName" type="ns1:String255" minOccurs="0"/>
					<xs:element name="code" type="tns:Code" minOccurs="0"/>
					<xs:element name="code3" type="tns:Code3" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CountryList">
		<xs:complexContent>
			<xs:extension base="ns1:EntityList">
				<xs:sequence>
					<xs:element name="country" type="tns:Country" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="District">
		<xs:complexContent>
			<xs:extension base="tns:AddressObjectView">
				<xs:sequence>
					<xs:element name="regionGuid" type="ns1:UUID" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DistrictList">
		<xs:complexContent>
			<xs:extension base="ns1:EntityList">
				<xs:sequence>
					<xs:element name="district" type="tns:District" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FederalDistrict">
		<xs:complexContent>
			<xs:extension base="ns1:GenericVersioningEntity">
				<xs:sequence>
					<xs:element name="fullName" type="ns1:String255" minOccurs="0"/>
					<xs:element name="shortName" type="ns1:String255" minOccurs="0"/>
					<xs:element name="abbreviation" type="ns1:String255" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Locality">
		<xs:complexContent>
			<xs:extension base="tns:AddressObjectView">
				<xs:sequence>
					<xs:element name="regionGuid" type="ns1:UUID" minOccurs="0"/>
					<xs:element name="districtGuid" type="ns1:UUID" minOccurs="0"/>
					<xs:element name="cityGuid" type="ns1:UUID" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="LocalityList">
		<xs:complexContent>
			<xs:extension base="ns1:EntityList">
				<xs:sequence>
					<xs:element name="locality" type="tns:Locality" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Region">
		<xs:complexContent>
			<xs:extension base="tns:AddressObjectView"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RegionList">
		<xs:complexContent>
			<xs:extension base="ns1:EntityList">
				<xs:sequence>
					<xs:element name="region" type="tns:Region" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Street">
		<xs:complexContent>
			<xs:extension base="tns:AddressObjectView">
				<xs:sequence>
					<xs:element name="localityGuid" type="ns1:UUID" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="StreetList">
		<xs:complexContent>
			<xs:extension base="ns1:EntityList">
				<xs:sequence>
					<xs:element name="street" type="tns:Street" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>